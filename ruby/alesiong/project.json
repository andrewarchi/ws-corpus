{
  "id": "ruby/alesiong",
  "name": "whitespace-ruby",
  "authors": ["alesiong"],
  "license": "GPL-2.0-or-later",
  "languages": ["Ruby"],
  "tags": ["program builder", "interpreter", "programs"],
  "date": "2016-03-05 16:39:05 +0800",
  "spec_version": "0.3",
  "source": ["https://github.com/alesiong/whitespace-ruby"],
  "submodules": [{ "path": "whitespace-ruby", "url": "https://github.com/alesiong/whitespace-ruby" }],
  "relations": [{ "id": "ruby/wconrad", "type": "fork" }],
  "whitespace": { "extension": "ws" },
  "assembly": {
    "mnemonics": {
      "push": ["push", "push"],
      "dup": ["dup", "dup"],
      "copy": ["", "dupnum"],
      "swap": ["swap", "swap"],
      "drop": ["discard", "discard"],
      "slide": ["", "discardnum"],
      "add": ["add", "add"],
      "sub": ["sub", "sub"],
      "mul": ["mul", "mul"],
      "div": ["div", "div"],
      "mod": ["mod", "mod"],
      "store": ["store", "store"],
      "retrieve": ["load", "retrieve"],
      "label": ["label", "label"],
      "call": ["call", "call"],
      "jmp": ["jump", "jump"],
      "jz": ["jz", "jz"],
      "jn": ["jn", "jn"],
      "ret": ["ret", "ret"],
      "end": ["exit", "exit"],
      "printc": ["printchar", "outchar"],
      "printi": ["printnum", "outnum"],
      "readc": ["readchar", "readchar"],
      "readi": ["readnum", "readnum"]
    },
    "usage": ["program builder", "interpreter"],
    "notes": "Mnemonics from wsv.rb and whitespace.rb, respectively"
  },
  "programs": [
    {
      "path": "sample/facws.rb",
      "generated": "fac.ws",
      "spec_version": "0.2",
      "generate": "ruby sample/facws.rb"
    },
    {
      "path": "sample/fibws.rb",
      "generated": "fib.ws",
      "spec_version": "0.2",
      "generate": "ruby sample/fibws.rb"
    }
  ],
  "commands": [
    {
      "type": "program builder",
      "bin": "wsv.rb",
      "notes": "Import into Ruby program to translate to Whitespace"
    },
    { "type": "interpreter", "bin": "whitespace.rb" }
  ]
}
