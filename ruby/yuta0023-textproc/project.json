{
  "id": "ruby/yuta0023-textproc",
  "name": "TextProc",
  "authors": ["YutaYamaguchi"],
  "license": "none",
  "languages": ["Ruby"],
  "tags": ["interpreter"],
  "date": "2015-11-16 15:17:05 +0900",
  "spec_version": "0.2",
  "source": ["https://github.com/yuta0023/TextProc"],
  "submodules": [{ "path": "TextProc", "url": "https://github.com/yuta0023/TextProc" }],
  "whitespace": { "extension": "ws" },
  "assembly": {
    "mnemonics": {
      "push": "push",
      "dup": "dup",
      "swap": "swap",
      "drop": "discard",
      "add": "add",
      "sub": "sub",
      "mul": "mul",
      "div": "div",
      "mod": "mod",
      "store": "store",
      "retrieve": "retrive",
      "label": "label",
      "call": "call",
      "jmp": "jump",
      "jz": "jz",
      "jn": "jn",
      "ret": "ret",
      "end": "exit",
      "printc": "outchar",
      "printi": "outnum",
      "readc": "readchar",
      "readi": "readnum"
    },
    "usage": ["enum"]
  },
  "programs": [
    { "path": "fibonacci.ws", "equivalent": "fibonacci.ws", "spec_version": "0.2" },
    { "path": "hanoi.ws", "equivalent": "hanoi.ws", "spec_version": "0.2" }
  ],
  "commands": [
    {
      "type": "interpreter",
      "bin": "mywhitespace.rb",
      "usage": "[<file>...]",
      "input": "concat:<file>... or stdin",
      "output": "stdout"
    }
  ]
}
