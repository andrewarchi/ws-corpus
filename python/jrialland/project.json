  "assembly": {
    "mnemonics": {
      "push": ["push"],
      "dup": ["dup"],
      "copy": ["copy"],
      "swap": ["swap"],
      "drop": ["pop"],
      "add": ["add"],
      "sub": ["sub"],
      "mul": ["mul"],
      "div": ["div"],
      "mod": ["mod"],
      "store": ["store"],
      "retrieve": ["load"],
      "label": ["label"],
      "call": ["call"],
      "jmp": ["jump"],
      "jz": ["jz"],
      "jn": ["jn"],
      "ret": ["ret"],
      "end": ["exit"],
      "printc": ["outchar"],
      "printi": ["outnum"],
      "readc": ["readchar"],
      "readi": ["readnum"]
    },
    "case_sensitive_instructions": true,
    "line_comments": ["#"],
    "usage": ["assembler"]
  },
  "commands": [
    {
      "type": "assembler",
      "bin": "whitespaceasm.py",
      "usage": "[<file>...] <last_file>",
      "output": "<last_file_replace:/\\.wsa$/.ws/>",
      "notes": "Multiple files are concatenated. If the last file does not end with .wsa, it will be overwritten."
    }
  ],
  "notes": "slide is not supported"
}
