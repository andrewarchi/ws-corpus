      "div": "ARITHMETIC_DIVISION",
      "mod": "ARITHMETIC_MODULO",
      "store": "HEAP_STORE",
      "retrieve": "HEAP_RETRIEVE",
      "label": "FLOW_MARK",
      "call": "FLOW_CALL_SUBROUTINE",
      "jmp": "FLOW_JUMP",
      "jz": "FLOW_ZJUMP",
      "jn": "FLOW_NJUMP",
      "ret": "FLOW_RETURN",
