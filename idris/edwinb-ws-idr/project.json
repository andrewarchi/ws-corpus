      "sub": "SUB",
      "mul": "MUL",
      "div": "DIV",
      "mod": "MOD",
      "store": "STORE",
      "retrieve": "RETRIEVE",
      "label": "LABEL",
      "call": "CALL",
      "jmp": "JUMP",
      "jz": "JZ",
      "jn": "JNEG",
      "ret": "RETURN",
      "end": "END",
      "printc": "OUTPUT",
      "printi": "OUTPUTNUM",
      "readc": "READCHAR",
      "readi": "READNUM"
    },
    "usage": ["enum", "debug"]
  },
  "programs": [
    { "path": "fact.ws", "equivalent": "fact.ws", "spec_version": "0.2" },
    { "path": "hanoi.ws", "equivalent": "hanoi.ws", "spec_version": "0.2" },
    { "path": "hworld.ws", "equivalent": "hworld.ws", "spec_version": "0.2" },
