{
  "id": "rust/nareshganduri",
  "name": "Whitespace VM",
  "authors": ["Naresh Ganduri"],
  "license": "none",
  "languages": ["Rust"],
  "tags": ["interpreter"],
  "date": "2019-05-13 03:46:04 -0500",
  "spec_version": "0.3",
  "source": ["https://github.com/nareshganduri/WhitespaceVM"],
  "submodules": [{ "path": "WhitespaceVM", "url": "https://github.com/nareshganduri/WhitespaceVM" }],
  "whitespace": { "extension": "ws" },
  "assembly": {
    "mnemonics": {
      "push": "Push",
      "dup": "Dup",
      "copy": "Copy",
      "swap": "Swap",
      "drop": "Pop",
      "slide": "Slide",
      "add": "Add",
      "sub": "Subtract",
      "mul": "Multiply",
      "div": "Divide",
      "mod": "Modulo",
      "store": "Store",
      "retrieve": "Retrieve",
      "call": "Call",
      "jmp": "Jump",
      "jz": "JumpIfZero",
      "jn": "JumpIfNeg",
      "ret": "Return",
      "end": "End",
      "printc": "OutputChar",
      "printi": "OutputNum",
      "readc": "ReadChar",
      "readi": "ReadNum"
    },
    "usage": ["enum"]
  },
  "programs": [
    { "path": "examples/calc.ws", "equivalent": "calc.ws", "spec_version": "0.2" },
    { "path": "examples/count.ws", "equivalent": "count.ws", "spec_version": "0.2" },
    { "path": "examples/fact.ws", "equivalent": "fact.ws", "spec_version": "0.2" },
    { "path": "examples/fibonacci.ws", "equivalent": "fibonacci.ws", "spec_version": "0.2" },
    { "path": "examples/hanoi.ws", "equivalent": "hanoi.ws", "spec_version": "0.2" },
    { "path": "examples/hworld.ws", "equivalent": "hworld.ws", "spec_version": "0.2" },
    { "path": "examples/loctest.ws", "equivalent": "loctest.ws", "spec_version": "0.3" },
    { "path": "examples/name.ws", "equivalent": "name.ws", "spec_version": "0.2" },
    { "path": "examples/sudoku.ws", "equivalent": "sudoku.ws", "spec_version": "0.3" }
  ],
  "commands": [{ "type": "interpreter", "bin": "target/release/whitespace-vm", "usage": "<file>" }]
}
